{
    "name": "indevops/calendar",
    "type": "roundcube-plugin",
    "description": "A calendar module for Roundcube. Fork of Texas Edition, which in turn is a fork of the original Kolab calendar.",
    "homepage": "https://github.com/INDEVOPS/roundcube_calendar",
    "license": "GPL-3.0-only",
    "version": "0.7",
    "authors": [{
            "name": "Thomas Bruederli",
            "email": "bruederli@kolabsys.com",
            "role": "Lead"
        },
        {
            "name": "Aleksander Machniak",
            "email": "machniak@kolabsys.com",
            "role": "Developer"
        },
        {
            "name": "Gene Hawkins",
            "email": "texxasrulez@yahoo.com",
            "role": "Developer"
        },
        {
            "name": "INDEVOPS",
            "email": "it@indevops.com",
            "role": "Developer"
        }
    ],
    "repositories": [{
        "type": "composer",
        "url": "https://plugins.roundcube.net"
    }],
    "require": {
        "php": ">=7.4.0",
        "roundcube/plugin-installer": ">=0.1.3",
        "sabre/dav": "3.0.9",
        "kolab/libkolab": "3.5.1",
        "indevops/libcalendaring": ">=0.6"
    },
    "autoload": {
        "psr-4": { "Calendar\\": "lib/" }
    },
    "extra": {
        "roundcube": {
            "min-version": "1.4.0",
            "sql-dir": "drivers/caldav/SQL"
        }
    }
}
